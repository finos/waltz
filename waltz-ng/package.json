{
    "name": "waltz-ng",
    "version": "1.74.0",
    "description": "To start test server run: `npm run dev-server` and ensure java backend is running on 8443",
    "main": "client/main.js",
    "browserslist": [
        "ie 11"
    ],
    "scripts": {
        "build": "webpack --config webpack.prod.js --progress --profile",
        "build-debug": "webpack --config webpack.dev.js --progress --profile",
        "dev-server": "webpack-dev-server --config webpack.dev.js --progress  --port 8000 --host 0.0.0.0",
        "postinstall": "patch-package",
        "test": "mocha --require @babel/register test/**/*.js --reporter=list > mocha-test-results.txt",
        "test-and-build": "npm test && webpack --config webpack.prod.js --progress --profile",
        "test-server": "BUILD_ENV=test webpack-dev-server --config webpack.config.js --progress --port 8000 --host 0.0.0.0",
        "watch": "webpack --progress --watch --colors",
        "watch-test": "npm test -- --watch",
        "help": "webpack --help=verbose"
    },
    "dependencies": {
        "@babel/polyfill": "7.12.1",
